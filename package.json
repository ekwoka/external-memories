{
  "name": "eleventy-base-blog",
  "version": "5.0.2",
  "description": "A starter repository for a blog web site using the Eleventy static site generator.",
  "scripts": {
    "start": "npm-run-all --parallel css eleventy",
    "eleventy": "eleventy --serve",
    "debug": "set DEBUG=* & eleventy",
    "css": "postcss css/tailwind.css --o _tmp/css/style.css --watch",
    "build": "cross-env NODE_ENV=production eleventy && cross-env NODE_ENV=production postcss css/tailwind.css --o _site/css/style.css"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/11ty/eleventy-base-blog.git"
  },
  "author": {
    "name": "Zach Leatherman",
    "email": "zachleatherman@gmail.com",
    "url": "https://zachleat.com/"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/11ty/eleventy-base-blog/issues"
  },
  "homepage": "https://github.com/11ty/eleventy-base-blog#readme",

  "dependencies": {
	  "lazysizes": "^5.3.0",
    "postcss": "^8.2.8",
    "autoprefixer": "^10.2.5"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.11.1",
    "@11ty/eleventy-navigation": "^0.1.6",
    "@11ty/eleventy-plugin-rss": "^1.1.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.1.0",
    "luxon": "^1.26.0",
    "markdown-it": "^12.0.4",
    "markdown-it-anchor": "^7.1.0",
    "eleventy-plugin-respimg": "^0.1.2",
	  "@tomleesm/markdown-it-wikilinks": "^1.1.1",
    "npm-run-all": "^4.1.5",
    "cross-env": "^7.0.3",
    "cssnano": "^4.1.10",
    "@tailwindcss/typography": "^0.4.0",
    "postcss-cli": "^8.3.1",
    "tailwindcss": "^2.0.3"
  }
}
