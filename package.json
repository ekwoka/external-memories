{
  "name": "eleventy-base-blog",
  "version": "5.0.2",
  "description": "A starter repository for a blog web site using the Eleventy static site generator.",
  "scripts": {
    "build": "npm-run-all build:css build:html",
    "build:css": "postcss css/tailwind.css -o ./_tmp/css/style.css",
    "build:html": "eleventy",
    "watch": "npm-run-all --parallel watch:css watch:html",
    "watch:css": "postcss css/tailwind.css -o ./_tmp/css/style.css --watch",
    "watch:html": "eleventy --serve"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/11ty/eleventy-base-blog.git"
  },
  "author": {
    "name": "Zach Leatherman",
    "email": "zachleatherman@gmail.com",
    "url": "https://zachleat.com/"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/11ty/eleventy-base-blog/issues"
  },
  "homepage": "https://github.com/11ty/eleventy-base-blog#readme",

  "dependencies": {
	  "lazysizes": "^5.2.2",
    "postcss": "^8.2.1",
    "autoprefixer": "^10.1.0",
    "@11ty/eleventy": "^0.11.0",
    "@11ty/eleventy-navigation": "^0.1.6",
    "@11ty/eleventy-plugin-rss": "^1.0.9",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.0.1",
    "luxon": "^1.21.3",
    "markdown-it": "^8.4.2",
    "markdown-it-anchor": "^5.2.5",
    "eleventy-plugin-respimg": "^0.1.2",
	  "@tomleesm/markdown-it-wikilinks": "^1.1.1",
    "npm-run-all": "^4.1.5",
    "cross-env": "^7.0.2",
    "cssnano": "^4.1.10",
    "@tailwindcss/typography": "^0.3.1",
    "postcss-cli": "^8.3.1",
    "tailwindcss": "^2.0.2"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.11.0",
    "@11ty/eleventy-navigation": "^0.1.6",
    "@11ty/eleventy-plugin-rss": "^1.0.9",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.0.1",
    "luxon": "^1.21.3",
    "markdown-it": "^8.4.2",
    "markdown-it-anchor": "^5.2.5",
    "eleventy-plugin-respimg": "^0.1.2",
	  "@tomleesm/markdown-it-wikilinks": "^1.1.1",
    "npm-run-all": "^4.1.5",
    "cross-env": "^7.0.2",
    "cssnano": "^4.1.10",
    "@tailwindcss/typography": "^0.3.1",
    "postcss-cli": "^8.3.1",
    "tailwindcss": "^2.0.2"
  }
}
